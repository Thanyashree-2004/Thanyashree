#include <stdio.h>
int main()
{
int i;
printf("the alpahbets from A-Z: ");
for(i=65;i<=90;i++){
    printf("%c",i);
}
printf("\nthe alphabets from a-z are: ");
for(i=97;i<=122;i++){
    printf("%c",i);
}
}



o/p:
the alpahbets from A-Z: ABCDEFGHIJKLMNOPQRSTUVWXYZ
the alphabets from a-z are: abcdefghijklmnopqrstuvwxyz


#include <stdio.h>
int main()
{
char i;
printf("the alpahbets from A-Z: ");
for(i='A';i<='Z';i++){
    printf("%c",i);
}
printf("\nthe alphabets from a-z are: ");
for(i='a';i<='z';i++){
    printf("%c",i);
}
}

o/p:
the alpahbets from A-Z: ABCDEFGHIJKLMNOPQRSTUVWXYZ
the alphabets from a-z are: abcdefghijklmnopqrstuvwxyz




#include <stdio.h>
int main()
{
char i;
i='a';
printf("the alpahbets from a-z  are:");
while(i<='z'){
    printf("%c",i);
    i++;
}
i='A';
printf("\nthe alphabets from A-Z are:");
while(i<='Z'){
    printf("%c",i);
    i++;
}
}



o/p:
the alpahbets from a-z  are:abcdefghijklmnopqrstuvwxyz
the alphabets from A-Z are:ABCDEFGHIJKLMNOPQRSTUVWXYZ
