#include <stdio.h>
int main()
{
int i=1,n=10,sum=0;
while(i<=n){
    sum+=i;
    i++;
}
printf("the sum of the naturall number is %d",sum);
}



o/p:
the sum of the naturall number is 55


#include <stdio.h>
int main()
{
int i,n=10,sum=0;
for(i=1;i<=n;i++){
    sum+=i;
    
}
printf("the sum of the naturall number is %d",sum);
}
#include <stdio.h>
int main()
{
int i,n=10,sum=0;
for(i=1;i<=n;i++){
    sum+=i;
    
}
printf("the sum of the naturall number is %d",sum);
}


o/p:
the sum of the naturall number is 55
